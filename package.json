{
  "name": "elm-symfony-bridge",
  "version": "1.0.4",
  "author": "Matthias Devlamynck <matthias.devlamynck@mailoo.fr>",
  "license": "MIT",
  "homepage": "https://github.com/mdevlamynck/elm-symfony-bridge#readme",
  "description": "Webpack plugin exposing symfony's translations and routing to elm.",
  "keywords": [
    "elm",
    "symfony",
    "tools",
    "webpack",
    "webpack-plugin",
    "translation",
    "routing",
    "bridge"
  ],
  "main": "dist/elm-symfony-bridge.js",
  "files": [
    "dist/"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "webpack",
    "test": "elm-test",
    "ci-format": "elm-format --validate .",
    "ci-test": "elm-test --verbose",
    "ci-test-examples": "elm-verify-examples --elm-test-args=\"--verbose\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdevlamynck/elm-symfony-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/mdevlamynck/elm-symfony-bridge/issues"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "schema-utils": "0.4.7"
  },
  "devDependencies": {
    "elm": "^0.19.0-bugfix6",
    "elm-format": "^0.8.1",
    "elm-test": "^0.19.0-rev6",
    "elm-verify-examples": "^3.1.0",
    "elm-webpack-loader": "^5.0.0",
    "path": "^0.12.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
